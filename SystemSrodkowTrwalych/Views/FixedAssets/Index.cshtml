@model IEnumerable<SystemSrodkowTrwalych.Models.FixedAssets>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Categories.CatTypes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.FirstName)
        </th>
     
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfCollections)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
      
        <td>
            @Html.DisplayFor(modelItem => item.Categories.CatTypes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Users.FirstName)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfCollections)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
